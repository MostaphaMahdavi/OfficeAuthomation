<!DOCTYPE html>

<html class="loading" lang="en" data-textdirection="rtl">
<!-- BEGIN: Head-->
<head>

    <partial name="PartialComponent/_meta" />

    <title>پنل ادمین -@ViewData["Title"]</title>
    <partial name="PartialComponent/_style" />
    <partial name="PartialComponent/_kendoStyleAndScripts" />
    <link href="~/css/customeAdmin.css" rel="stylesheet" />
    @RenderSection("Styles", false)

</head>
<!-- END: Head-->
<!-- BEGIN: Body-->
<body class="vertical-layout vertical-menu-modern 2-columns  navbar-floating footer-static   menu-collapsed" data-open="click" data-menu="vertical-menu-modern" data-col="2-columns">

    <!-- BEGIN: Header-->
    <partial name="PartialComponent/_header" />
    <!-- END: Header-->
    <!-- BEGIN: Main Menu-->
    <partial name="AdminPartialComponent/_adminMenu" />
    <!-- END: Main Menu-->
    <!-- BEGIN: Content-->
    <div class="app-content content">
        <div class="content-overlay"></div>
        <div class="header-navbar-shadow"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <!-- Dashboard Analytics Start -->
                @RenderBody()
                <!-- Dashboard Analytics end -->

            </div>
        </div>
    </div>
    <!-- END: Content-->
    <!-- BEGIN: Customizer-->
    <partial name="PartialComponent/_customizer" />
    <!-- End: Customizer-->
    <!-- Buynow Button-->

    <partial name="PartialComponent/_byeButton" />
    <!-- BEGIN: Footer-->
    <partial name="PartialComponent/_footer" />
    <!-- END: Footer-->


    <partial name="PartialComponent/_script" />

    @RenderSection("Scripts", false)
</body>
<!-- END: Body-->
</html>